
function float_div_float(a: Float, b: Float) :: Float | NaN => a / b
function nonzero_div_Float(a: != 0.0, b: Float) :: Float => a / b
function float_div_NonZero(a: Float, b: != 0.0) :: Float => a / b
function positive_mod_10(a: >= 0) :: 0 .. 10 => a % 10

function one_to_four_div_neg_four_to_four(a: 1.0 .. 4.0, b: -4.0 .. 4.0) :: <= -0.25 | > 0.25
    return a / b

function for_loop_result_positive(a: Integer) :: >= 0
    var total: >= 0 = 0
    for i in 0 .. a
        total += i
    return total

function divide_non_zero(a: -1.0 .. 5.0, b: -2.0 .. 2.0) :: Float
    if b == 0.0
        return - 10.0
    return a / b

function conditionals_add(a: -1.0 .. 5.0, b: -2.0 .. 2.0) :: -20.0 | -10.0 | -3.0 .. 5.0
    if b == 0.0
        return -10.0
    if b > 0.0
        return -20.0
    return a + b
